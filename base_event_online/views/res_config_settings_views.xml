<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <data>
        <record id="res_config_settings_event_base" model="ir.ui.view">
            <field name="name">res.conf.odoo.onlin.event</field>
            <field name="priority" eval="72" />
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="base.res_config_settings_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//div[hasclass('settings')]" position="inside">
                    <div class="app_settings_block" name="bbutton" data-key="base_event_online" data-string="Online Events">
                        <div class="app_settings_block_evl_events_zoom"></div>
                        <div class="app_settings_block_evl_events_bigbluebutton"></div>

                    </div>
                </xpath>
            </field>
        </record>

    </data>


</odoo>